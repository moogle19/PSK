program aufg4;
const
    x51 = 'b';
    x52 = 20;
    x7  = 1;
    x8  = 10;
    x10 = 'C';
var
    x1  : boolean;
    x2  : integer;
    x3  : real;
    x41 : char;
    x42 : integer;
    x6  : boolean;
    x9  : real;
    x11 : real;
    x12 : real;
begin
    x1  := odd(x2) < (sqrt(x3) >= 3.8);
    x41 := pred(x51);
    x42 := pred(x52);
    x6  := chr(sqr(abs(x7 * x8))) <> 'H';
    x9  := ord(x10) * x11 / x12
end.
